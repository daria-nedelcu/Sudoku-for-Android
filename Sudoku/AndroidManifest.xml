<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sudoku"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.sudoku.MenuSudoku"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.sudoku.Difficulty"
            android:label="@string/title_activity_difficulty" >
        </activity>
        <activity
            android:name="com.example.sudoku.Howtoplay"
            android:label="@string/title_activity_howtoplay" >
        </activity>
        <activity
            android:name="com.example.sudoku.Congrats"
            android:label="@string/title_activity_congrats" >
        </activity>
        <activity
            android:name="com.example.sudoku.SudokuEasyGame"
            android:label="@string/title_activity_sudoku_easy_game" >
        </activity>
        <activity
            android:name="com.example.sudoku.SudokuMediumGame"
            android:label="@string/title_activity_sudoku_medium_game" >
        </activity>
        <activity
            android:name="com.example.sudoku.SudokuEvilGame"
            android:label="@string/title_activity_sudoku_evil_game" >
        </activity>
    </application>

</manifest>
